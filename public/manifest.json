{
  "name": "Better WFH",
  "description": "A tool for better handling of WFH in central",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Better WFH",
    "default_icon": "vite.svg",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}